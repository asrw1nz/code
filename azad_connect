#Auth to the proxy
"Auth Proxy"
$webclient=New-Object System.Net.WebClient
$creds=Get-Credential
$webclient.Proxy.Credentials=$creds

#login to the Azure tenant
"Connect AzureRM"
Login-AzureRmAccount

#login to the Azure tenant
"Connect AzureAD"
Connect-AzureAD


